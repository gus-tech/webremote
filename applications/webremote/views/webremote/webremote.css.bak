body{
    margin:0;
    padding:0;
    border:0;
    background-color:lightgray;
}


.button_label{
    margin-left: auto;
    margin-right: auto;
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}

.button{
    border: 1px solid black;
    border-radius: 10px;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    position: relative;
    z-index: 100;
}
.template{
    display: inline-block;
    background-color:pink;
    position: relative;
    cursor: move;
    margin: 10px;
}

.template_mobile{
    cursor: copy;
}

#settings_div{
    text-align: center;
    display: inline-block;
    padding: 10px;
    border: 0;
    margin: 0;
}

.settings_div{
    text-align: center;
    min-width:70px;
    min-height:20px;
    max-width:70px;
    max-height:70px;
    display: inline-block;
}

.power{
    background-color: white;
    width:70px;
    height:20px;
    min-width:70px;
    min-height:20px;
    max-width:70px;
    max-height:20px;
}


hr{
    border-width: 3px;
}

.check{
   cursor: pointer;
  /* Double-sized Checkboxes */
  -ms-transform: scale(2); /* IE */
  -moz-transform: scale(2); /* FF */
  -webkit-transform: scale(2); /* Safari and Chrome */
  -o-transform: scale(2); /* Opera */
  display: inline-block;
}

.clickable{
    box-shadow: 0 6px #999;
}
.clickable:active {
    box-shadow: 0 3px #666;
    transform: translateY(3px);
}
.small{
    width:40px;
    height:40px;
    min-width:40px;
    min-height:40px;
    max-width:40px;
    max-height:40px;
}
.large{
    width:100px;
    height:100px;
    min-width:100px;
    min-height:100px;
    max-width:100px;
    max-height:100px;
}
.wide{
    width:100px;
